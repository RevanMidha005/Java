package P1;

import java.util.*;

public class sumavg {
	
	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		
		double a, b, c, d, e, s, avg;
		
		a = scn.nextInt();
		b = scn.nextInt();
		c = scn.nextInt();
		d = scn.nextInt();
		e = scn.nextInt();
		
		sumavrg obj = new sumavrg();
		
		s = obj.sum(a, b, c, d, e);
		avg = obj.avg(s);
		
		System.out.println("Sum of Numbers: "+s);
		System.out.println("Average of Numbers: "+avg);
		
		scn.close();
	}
}
